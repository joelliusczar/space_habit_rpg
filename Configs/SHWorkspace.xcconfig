SUPPORTED_PLATFORMS = iphonesimulator iphoneos macosx
//CONFIGURATION is Release, Debug, etc

SH_COMMON_BUILD_ROOT = $(SRCROOT)/../../spacehabitrpg_output/build/$(PLATFORM_NAME)

// Intermediate build files go here
PROJECT_TEMP_DIR = $(SH_COMMON_BUILD_ROOT)/$(PROJECT_NAME).build
//
//// Build-related files for the active build configuration go here
CONFIGURATION_BUILD_DIR = $(SH_COMMON_BUILD_ROOT)/$(CONFIGURATION)
//
//// The final product executables and other build products go here
BUILT_PRODUCTS_DIR = $(SH_COMMON_BUILD_ROOT)/$(CONFIGURATION)

